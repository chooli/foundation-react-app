{
  "name": "foundation-react-app",
  "version": "0.0.1",
  "description": "a foundation front-end application architecture for react",
  "main": "index.js",
  "scripts": {
    "build": "yarn test && yarn webpack",
    "start": "webpack-dev-server --mode development --open",
    "webpack": "npx --node-arg=--max_old_space_size=2048 webpack",
    "build:analyze": "yarn build --analyze",
    "build:production": "yarn build -p",
    "build:production-analyze": "yarn build -p --analyze",
    "dev": "yarn webpack --watch",
    "clean": "rimraf *.log src/main/resources/javascript/apps",
    "clean:all": "yarn clean && rimraf node_modules node",
    "test": "yarn lint",
    "lint": "eslint --ext js,jsx .",
    "lint:fix": "eslint --ext js,jsx --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chooli/foundation-react-app.git"
  },
  "keywords": [
    "foundation react"
  ],
  "author": "chooli yip <chooli.yip@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chooli/foundation-react-app/issues"
  },
  "homepage": "https://github.com/chooli/poc-react-app#readme",
